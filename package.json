{
  "name": "eslintboilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for JavaScript and TypeScript project from scratch, with ESLint/TSLint AirBNB Style.",
  "main": "app.js",
  "scripts": {
    "run": "parcel -p 4200 src/index.html",
    "build": "parcel build src/index.html --no-source-maps",
    "deploygithub": "parcel build src/index.html --no-source-maps --public-url /your-github-project-name/",
    "removedist": "rm -rf ./dist",
    "start-dev": "webpack-dev-server --config webpack.config.dev.js --open",
    "start-prod": "webpack-dev-server --config webpack.config.prod.js --open",
    "build-prod": "npm run removedist && webpack --config webpack.config.prod.js",
    "test-prod": "http-server ./dist -p 4200"
  },
  "keywords": [
    "boilerplate"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "author": "gulajava_ministudio",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.2",
    "css-minimizer-webpack-plugin": "^1.1.2",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.10.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "typescript": "^3.9.7",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2"
  }
}
